*.class binary
*.gif   binary
*.eot   binary
*.ico   binary
*.jpg   binary
*.map   binary
*.ttf   binary
*.woff  binary
*.woff2 binary

# Enforce Unix newlines
*.css   text eol=lf
*.html  text eol=lf
*.js    text eol=lf
*.json  text eol=lf
*.less  text eol=lf
*.md    text eol=lf
*.svg   text eol=lf
*.yml   text eol=lf

*.java  diff=java
*.scala diff=java
